{
  "name": "atom-lupa",
  "main": "./lib/atom-lupa",
  "__main": "./dist/bundle.js",
  "version": "0.4.90",
  "description": "Lupa 🔍  (from Polish: 'magnifying glass'). Know your codebase.",
  "scripts": {
    "reinstall": "rm -rf node_modules; npm install; webpack"
  },
  "keywords": [
    "angular",
    "modules",
    "dependencies",
    "navigate",
    "navigation",
    "navigator",
    "imports",
    "require",
    "minimap",
    "angularjs",
    "structure",
    "static analysis",
    "ast",
    "outline",
    "overview",
    "summary",
    "go to definition",
    "todo",
    "todos",
    "react",
    "reactjs",
    "jsx"
  ],
  "__providedServices": {
    "autocomplete.provider": {
      "versions": {
        "2.0.0": "provide"
      }
    }
  },
  "activationCommands": {
    "atom-workspace": "atom-lupa:toggle"
  },
  "repository": "https://github.com/hex13/atom-lupa",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "ast-stream": "0.0.15",
    "coffee-script": "^1.10.0",
    "flow-parser": "^0.21.0",
    "html-flavors": "0.0.20",
    "lupa": "0.1.66",
    "react": "^15.0.1",
    "react-dom": "^15.0.1",
    "through2": "^2.0.1",
    "vinyl": "^1.1.1",
    "vinyl-fs": "^2.4.3"
  },
  "devDependencies": {
    "eslint": "^2.8.0",
    "tape": "^4.4.0"
  }
}
